    "dependencies": {
        "@adobe/aio-lib-ims": "^7.0.2",
        "@adobe/aio-lib-state": "^5.1.0",
        "@adobe/aio-sdk": "^6",
        "@adobe/exc-app": "^1.3.0",
        "cloudevents": "^4.0.2",
        "node-fetch": "^2.6.0",
        "regenerator-runtime": "^0.13.5",
        "uuid": "^8.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-terser": "^0.4.4",
        "c8": "^10.1.3",
        "chai": "^4.3.6",
        "eslint": "^8",
        "esm": "^3.2.25",
        "mocha": "^10.2.0",
        "nock": "^13.0.0",
        "rollup": "^3.29.4"
    },
    "scripts": {
        "test": "c8 --check-coverage --lines=99 --functions=99 --branches=99 --statements=99 --exclude=src/fragment/odinSchemaTransform.js --exclude=src/ost-products/read.js --exclude=src/ost-products/write.js --exclude=test/**/* mocha --recursive ./test",
        "test:watch": "mocha --watch --recursive ./test",
        "test:file": "mocha --watch --recursive --grep",
        "test:coverage": "c8 --reporter=lcov --reporter=text-summary --check-coverage --lines=99 --functions=99 --branches=99 --statements=99 --exclude=src/fragment/odinSchemaTransform.js --exclude=src/ost-products/read.js --exclude=src/ost-products/write.js --exclude=test/**/* mocha --recursive ./test",
        "lint": "eslint --ignore-pattern web-src --no-error-on-unmatched-pattern test src actions",
        "lint:fix": "npm run lint -- --fix",
        "build:client": "rollup -c rollup.config.js"
    },
    "engines": {
        "node": ">=22"
    }
}
